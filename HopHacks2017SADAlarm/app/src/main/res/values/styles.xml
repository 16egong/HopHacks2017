<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="PickerTheme"
        parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorControlNormal">@color/colorPrimary1</item>
        <item name="colorControlActivated">@color/colorAccent1</item>
        <item name="textColorAlertDialogListItem">@color/colorPrimaryDark1</item>
        <item name="colorAccent">@color/colorPrimary1</item>
        <item name="android:textColorPrimary">@color/colorPrimaryDark1</item>


    </style>
    



</resources>
